# Celda de código
numero1 = 5
numero2 = 3

# Suma
suma = numero1 + numero2
print("La suma es:", suma)

# Multiplicación
multiplicacion = numero1 * numero2
print("El resultado de la multiplicación es:", multiplicacion)


# Celda de código
minutos = 150

# Convertir minutos a horas
horas = minutos / 60
print("El equivalente de", minutos, "minutos en horas es:", horas)


# Celda de código
objetivos = ["Aprender sobre ciencia de datos", "Practicar con Jupyterlite Notebook", "Compartir conocimientos a través de GitHub"]
print("Objetivos del Proyecto Final:")
for objetivo in objetivos:
    print("-", objetivo)
